  "version": "0.2.0",
  "configurations": [
    {
      "name": "tabnews",
      "request": "launch",
      "type": "dart",
      "args": [
        "--enable-impeller"
      ]
    },
    {
      "name": "tabnews (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": [
        "--enable-impeller"
      ]
    },
    {
      "name": "tabnews (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "--enable-impeller"
      ]
    },
    {
      "name": "tabnews (no impeller)",
      "request": "launch",
      "type": "dart",
      "args": [
        "--no-enable-impeller"
      ]
    },
    {
      "name": "tabnews (profile mode) (no impeller)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": [
        "--no-enable-impeller"
      ]
    },
    {
      "name": "tabnews (release mode) (no impeller)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "--no-enable-impeller"
      ]
    }
  ]
}